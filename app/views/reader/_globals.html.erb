<% feed_id = "" if local_assigns[:feed_id].nil? %>
<% items_filter = "unread" if local_assigns[:items_filter].nil? %>
<% date_sort = "chron" if local_assigns[:date_sort].nil? %>

<script type="text/javascript">
	var SETTINGS = {
		date_sort: "<%= date_sort %>",
		items_filter: "<%= items_filter %>",
		feed_id: "<%= feed_id %>",
		page: 0
	};
	
	var MY_FEED_ID = "<%= current_user.feed.id %>";
	
	var INSTAPAPER = {username: "<%= current_user.instapaper_username %>", password: "<%= current_user.instapaper_password %>"}
  
  var USERS = <%= raw(User.select([:id, :fingerprint, :name, :email]).map { |u| {id: u.id, name: u.name, fingerprint: u.fingerprint, email: u.email} }.to_json) %>;
</script>